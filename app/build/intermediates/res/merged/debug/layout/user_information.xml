<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >
	<TextView 
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_x="10dp"
		android:layout_y="10dp"
		android:text="@string/tvAppId_text"
		android:textSize="20sp"/>
	<TextView 
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_x="140dp"
		android:layout_y="10dp"
		android:text="@string/tvTwoPoint_text"
		android:textSize="20sp"/>
	<EditText
	    android:id="@+id/etAppId" 
		android:layout_width="200dp"
		android:layout_height="wrap_content"
		android:layout_x="150dp"
		android:layout_y="5dp"
		android:singleLine="true"
		android:hint="@string/etAppId_text"
		android:textSize="20sp"/>
	
	<TextView 
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_x="10dp"
		android:layout_y="60dp"
		android:text="@string/tvMacAddr_text"
		android:textSize="20sp"/>
	<TextView 
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_x="140dp"
		android:layout_y="60dp"
		android:text="@string/tvTwoPoint_text"
		android:textSize="20sp"/>
	<EditText
	    android:id="@+id/etMacAddr" 
		android:layout_width="200dp"
		android:layout_height="wrap_content"
		android:layout_x="150dp"
		android:layout_y="55dp"
		android:hint="@string/etMacAddr_text"
		android:inputType="textCapCharacters"
		android:textSize="20sp"/>
	
    <TextView 
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_x="10dp"
		android:layout_y="110dp"
		android:text="@string/tvPhoneNum_text"
		android:textSize="20sp"/>
	<TextView
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_x="140dp"
		android:layout_y="110dp"
		android:text="@string/tvTwoPoint_text"
		android:textSize="20sp"/>
	<EditText
	    android:id="@+id/etPhoneNum" 
		android:layout_width="200dp"
		android:layout_height="wrap_content"
		android:layout_x="150dp"
		android:layout_y="105dp"
		android:hint="@string/etPhoneNum_text"
		android:inputType="phone"
		android:textSize="20sp"/>
	
	<Button
	    android:id="@+id/btResetToDefault" 
		android:layout_width="200dp"
		android:layout_height="wrap_content"
		android:layout_x="150dp"
		android:layout_y="155dp"
		android:text="@string/btResetToDefault_text"
		android:textSize="20sp"/>
	
	<TextView 
	    android:id="@+id/tvUserId"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_x="70dp"
		android:layout_y="250dp"
		android:text="@string/tvUserId_text"
		android:textSize="20sp"/>
	<TextView
	    android:id="@+id/tvTwoPointUser"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_x="140dp"
		android:layout_y="250dp"
		android:text="@string/tvTwoPoint_text"
		android:textSize="20sp"/>
	<EditText
	    android:id="@+id/etUserId" 
		android:layout_width="200dp"
		android:layout_height="wrap_content"
		android:layout_x="150dp"
		android:layout_y="245dp"
		android:hint="@string/etUserId_text"
		android:textSize="20sp"/>
	
	<TextView
	    android:id="@+id/tvPass"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_x="43dp"
		android:layout_y="300dp"
		android:text="@string/tvPassword_text"
		android:textSize="20sp"/>
	<TextView
	    android:id="@+id/tvTwoPointPass"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_x="140dp"
		android:layout_y="300dp"
		android:text="@string/tvTwoPoint_text"
		android:textSize="20sp"/>
	<EditText
	    android:id="@+id/etPassword" 
		android:layout_width="200dp"
		android:layout_height="wrap_content"
		android:layout_x="150dp"
		android:layout_y="295dp"
		android:hint="@string/etPassword_text"
		android:inputType="textPassword"
		android:textSize="20sp"/>
	
	<Button
	    android:id="@+id/btSet_User" 
		android:layout_width="100dp"
		android:layout_height="wrap_content"
		android:layout_x="60dp"
		android:layout_y="400dp"
		android:text="@string/btSet_text"
		android:textSize="20sp"/>	
	

	<Button
	    android:id="@+id/btCancel_User" 
		android:layout_width="100dp"
		android:layout_height="wrap_content"
		android:layout_x="190dp"
		android:layout_y="400dp"
		android:text="@string/btCancel_text"
		android:textSize="20sp"/>
	
	<Button
	    android:id="@+id/btBack_User" 
		android:layout_width="100dp"
		android:layout_height="wrap_content"
		android:layout_x="120dp"
		android:layout_y="400dp"
		android:text="@string/btBack_text"
		android:textSize="20sp"
		android:visibility="gone"/>
</AbsoluteLayout>
